  bottom: "double_chin_attr"
  top: "double_chin_attr"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "double_chin_score"
  type: "InnerProduct"
  bottom: "double_chin_attr"
  top: "double_chin_score"
  inner_product_param {
    num_output: 2
  }
}

layer {
  name: "double_chin_prob"
