  type: "InnerProduct"
  bottom: "part_shared_conv2"
  top: "chubby_attr"
  inner_product_param {
    num_output: 512
  }
}

layer {
  name: "chubby_relu"
  type: "ReLU"
  bottom: "chubby_attr"
  top: "chubby_attr"
}

layer {
  name: "chubby_dropout"
  type: "Dropout"
  bottom: "chubby_attr"
  top: "chubby_attr"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "chubby_score"
  type: "InnerProduct"
  bottom: "chubby_attr"
  top: "chubby_score"
  inner_product_param {
    num_output: 2
  }
}

layer {
  name: "chubby_prob"
  type: "Softmax"
  bottom: "chubby_score"
  top: "chubby_prob"
  loss_param {
    ignore_label: -1
    normalize: true
  }
}

#== group 9, loss of Double_Chin attr ==
layer {
  name: "double_chin_attr"
  type: "InnerProduct"
  bottom: "part_shared_conv2"
  top: "double_chin_attr"
