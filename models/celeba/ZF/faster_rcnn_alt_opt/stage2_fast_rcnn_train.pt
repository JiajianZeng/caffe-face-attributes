name: "ZF"
layer {
  name: 'data'
  type: 'Python'
  top: 'data'
  top: 'rois'
  top: 'rois_shuffled'
  top: 'labels_shuffled'
  top: 'face_attrs'
  python_param {
    module: 'roi_data_layer.layer'
    layer: 'RoIDataLayer'
    param_str: "'num_classes': 7"
  }
}

#========= face_attrs slice, of which the shape is N * 40 * 1 * 1 =========
layer {
  name: "face_attrs_slice"
  type: "Slice"
  bottom: "face_attrs"

  top: "5_o_Clock_Shadow_label"
  top: "Arched_Eyebrows_label"
  top: "Attractive_label"
  top: "Bags_Under_Eyes_label"
  top: "Bald_label"

  top: "Bangs_label"
  top: "Big_Lips_label"
  top: "Big_Nose_label"
  top: "Black_Hair_label"
  top: "Blond_Hair_label"

  top: "Blurry_label"
  top: "Brown_Hair_label"
  top: "Bushy_Eyebrows_label"
  top: "Chubby_label"
  top: "Double_Chin_label"

  top: "Eyeglasses_label"
  top: "Goatee_label"
  top: "Gray_Hair_label"
  top: "Heavy_Makeup_label"
  top: "High_Cheekbones_label"

  top: "Male_label"
  top: "Mouth_Slightly_Open_label"
  top: "Mustache_label"
  top: "Narrow_Eyes_label"
  top: "No_Beard_label"

  top: "Oval_Face_label"
  top: "Pale_Skin_label"
  top: "Pointly_Nose_label"
  top: "Receding_Hairline_label"
  top: "Rosy_Cheeks_label"

  top: "Sideburns_label"
  top: "Smiling_label"
  top: "Straight_Hair_label"
  top: "Wavy_Hair_label"
  top: "Wearing_Earrings_label"

  top: "Wearing_Hat_label"
  top: "Wearing_Lipstick_label"
  top: "Wearing_Necklace_label"
  top: "Wearing_Necktie_label"
  top: "Young_label"

  slice_param {
    axis: 1
    slice_point: 1
    slice_point: 2
    slice_point: 3
    slice_point: 4
    slice_point: 5

    slice_point: 6
    slice_point: 7
    slice_point: 8
    slice_point: 9
    slice_point: 10

    slice_point: 11
    slice_point: 12
    slice_point: 13
    slice_point: 14
    slice_point: 15

    slice_point: 16
    slice_point: 17
    slice_point: 18
    slice_point: 19
    slice_point: 20

    slice_point: 21
    slice_point: 22
    slice_point: 23
    slice_point: 24
    slice_point: 25

    slice_point: 26
    slice_point: 27
    slice_point: 28
    slice_point: 29
    slice_point: 30

    slice_point: 31
    slice_point: 32
    slice_point: 33
    slice_point: 34
    slice_point: 35

    slice_point: 36
    slice_point: 37
    slice_point: 38
    slice_point: 39

  }
}

#========= conv1-conv5 ============

layer {
  name: "conv1"
  type: "Convolution"
  bottom: "data"
  top: "conv1"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 96
    kernel_size: 7
    pad: 3
    stride: 2
  }
}
layer {
  name: "relu1"
  type: "ReLU"
  bottom: "conv1"
  top: "conv1"
}
layer {
  name: "norm1"
  type: "LRN"
  bottom: "conv1"
  top: "norm1"
  lrn_param {
    local_size: 3
    alpha: 0.00005
    beta: 0.75
    norm_region: WITHIN_CHANNEL
    engine: CAFFE
  }
}
layer {
  name: "pool1"
  type: "Pooling"
  bottom: "norm1"
  top: "pool1"
  pooling_param {
    kernel_size: 3
    stride: 2
    pad: 1
    pool: MAX
  }
}
layer {
  name: "conv2"
  type: "Convolution"
  bottom: "pool1"
  top: "conv2"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 256
    kernel_size: 5
    pad: 2
    stride: 2
  }
}
layer {
  name: "relu2"
  type: "ReLU"
  bottom: "conv2"
  top: "conv2"
}
layer {
  name: "norm2"
  type: "LRN"
  bottom: "conv2"
  top: "norm2"
  lrn_param {
    local_size: 3
    alpha: 0.00005
    beta: 0.75
    norm_region: WITHIN_CHANNEL
    engine: CAFFE
  }
}
layer {
  name: "pool2"
  type: "Pooling"
  bottom: "norm2"
  top: "pool2"
  pooling_param {
    kernel_size: 3
    stride: 2
    pad: 1
    pool: MAX
  }
}
layer {
  name: "conv3"
  type: "Convolution"
  bottom: "pool2"
  top: "conv3"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 384
    kernel_size: 3
    pad: 1
    stride: 1
  }
}
layer {
  name: "relu3"
  type: "ReLU"
  bottom: "conv3"
  top: "conv3"
}
layer {
  name: "conv4"
  type: "Convolution"
  bottom: "conv3"
  top: "conv4"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 384
    kernel_size: 3
    pad: 1
    stride: 1
  }
}
layer {
  name: "relu4"
  type: "ReLU"
  bottom: "conv4"
  top: "conv4"
}
layer {
  name: "conv5"
  type: "Convolution"
  bottom: "conv4"
  top: "conv5"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 256
    kernel_size: 3
    pad: 1
    stride: 1
  }
}
layer {
  name: "relu5"
  type: "ReLU"
  bottom: "conv5"
  top: "conv5"
}

#========= RCNN for roi class prediction ============

layer {
  name: "roi_pool_conv5_class_prediction"
  type: "ROIPooling"
  bottom: "conv5"
  bottom: "rois_shuffled"
  top: "roi_pool_conv5_class_prediction"
  roi_pooling_param {
    pooled_w: 6
    pooled_h: 6
    spatial_scale: 0.0625 # 1/16
  }
}

#========= loss of proposal cls =========
layer {
  name: "fc6"
  type: "InnerProduct"
  bottom: "roi_pool_conv5_class_prediction"
  top: "fc6"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  inner_product_param {
    num_output: 4096
    weight_filler { type: "xavier" }
  }
}
layer {
  name: "relu6"
  type: "ReLU"
  bottom: "fc6"
  top: "fc6"
}
layer {
  name: "drop6"
  type: "Dropout"
  bottom: "fc6"
  top: "fc6"
  dropout_param {
    dropout_ratio: 0.5
    scale_train: false
  }
}
layer {
  name: "fc7"
  type: "InnerProduct"
  bottom: "fc6"
  top: "fc7"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  inner_product_param {
    num_output: 4096
    weight_filler { type: "xavier" }
  }
}
layer {
  name: "relu7"
  type: "ReLU"
  bottom: "fc7"
  top: "fc7"
}
layer {
  name: "drop7"
  type: "Dropout"
  bottom: "fc7"
  top: "fc7"
  dropout_param {
    dropout_ratio: 0.5
    scale_train: false
  }
}
layer {
  name: "cls_score"
  type: "InnerProduct"
  bottom: "fc7"
  top: "cls_score"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  inner_product_param {
    num_output: 7
    weight_filler { type: "xavier" }
  }
}

layer {
  name: "loss_cls"
  type: "SoftmaxWithLoss"
  bottom: "cls_score"
  bottom: "labels_shuffled"
  top: "cls_loss"
  loss_param {
    ignore_label: -1
    normalize: true
  }
}

#========= RCNN for face attributes prediction ============

layer {
  name: "roi_pool_conv5"
  type: "ROIPooling"
  bottom: "conv5"
  bottom: "rois"
  top: "roi_pool_conv5"
  roi_pooling_param {
    pooled_w: 6
    pooled_h: 6
    spatial_scale: 0.0625 # 1/16
  }
}

#========= we perform a depth concat and part shared convolution =========
layer {
  name: "roi_pool_conv5_reshape"
  bottom: "roi_pool_conv5"
  top: "roi_pool_conv5_reshape"
  type: "Reshape"
  reshape_param {
    shape { dim: 8 dim: -1 dim: 0 dim: 0 }
  }
}

layer {
  name: "part_shared_conv1"
  bottom: "roi_pool_conv5_reshape"
  top: "part_shared_conv1"
  type: "Convolution"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 128
    kernel_size: 3 pad: 1 stride: 1
    weight_filler { type: "xavier" }
  }
}

layer{
  name: "part_shared_relu1"
  bottom: "part_shared_conv1"
  top: "part_shared_conv1"
  type: "ReLU"
}

layer {
  name: "part_shared_conv2"
  bottom: "part_shared_conv1"
  top: "part_shared_conv2"
  type: "Convolution"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 256
    kernel_size: 3 pad: 1 stride: 1
    weight_filler { type: "xavier" }
  }
}

layer{
  name: "part_shared_relu2"
  bottom: "part_shared_conv2"
  top: "part_shared_conv2"
  type: "ReLU"
}

#========= attribute group 1 - group 9 =========

#== group 1, loss of Male attr ==
layer {
  name: "male_attr"
  type: "InnerProduct"
  bottom: "part_shared_conv2"
  top: "male_attr"
  param {lr_mult: 1.0}
  param {lr_mult: 2.0}
