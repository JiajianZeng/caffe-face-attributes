  bottom: "male_attr"
  top: "male_score"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  inner_product_param {
    num_output: 2
    weight_filler { type: "xavier" }
  }
}

layer {
  name: "male_score_loss"
  type: "SoftmaxWithLoss"
  bottom: "male_score"
  bottom: "Male_label"
  top: "male_score_loss"
}

